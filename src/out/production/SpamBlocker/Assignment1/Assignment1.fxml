<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TextField?>
<GridPane fx:controller="Assignment1.Controller" xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">
    <TableView fx:id="tableView" GridPane.columnIndex="0" GridPane.rowIndex="0">
    <columns>
        <TableColumn fx:id="File" text="File"/>
        <TableColumn fx:id="ActualClass" text="Actual Class"/>
        <TableColumn fx:id="SpamProbability" text="SpamProbability"/>
    </columns>

    <Label text="Accuracy: " GridPane.columnIndex="0" GridPane.rowIndex="1" />
    <TextField fx:id="Accuracy" GridPane.columnIndex="1" GridPane.rowIndex="1" />
    <Label text="Precision: " GridPane.columnIndex="0" GridPane.rowIndex="2" />
    <TextField fx:id="Precision" GridPane.columnIndex="1" GridPane.rowIndex="2" />
</GridPane>